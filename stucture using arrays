#include <stdio.h>
struct student{
    int age;
    float marks;
};
int main() {
    struct student s[2];
    int i;
    for(i=0;i<2;i++){
   printf("enter the  student age & marks");
   scanf("%d %f",&s[i].age,&s[i].marks);
    }
    printf("student details:");
    for(i=0;i<2;i++){
   printf("age:%d marks:%f",s[i].age,s[i].marks);
}
}
