//addiont of matrix
#include<stdio.h>
int main() { 
    int  rows[2], col[3];
    int a[2][3],b[2][3],c[2][3];
    printf("enter elements of a:\n");
    for(int i=0;i<2;i++){
        for( int j=0;j<3;j++){
            scanf("%d",&a[i][j]);
        }
    }
        printf("enter elements of b :\n");
        for(int i=0;i<2;i++){
        for(int j=0;j<3;j++){
            scanf("%d",&b[i][j]);
        }
    }
         for(int i=0;i<2;i++){
             for(int j=0;i<3;i++){
                 c[i][j]=a[i][j]+b[i][j];
             }
         }
         printf("%d",&c[i][j]);
}
